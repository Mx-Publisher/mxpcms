<table width="{BLOCK_SIZE}" cellpadding="4" cellspacing="0" border="0" class="forumline">
<TR VALIGN="TOP"> 

<tr> 
  <th class="thHead" align="left" colspan="2">&nbsp;{RSS_TITLE}&nbsp;</th>
</tr> 

<!-- BEGIN rss_image -->
<tr> 
  <td class="row1" height="25">
    <a href="{rss_image.RSS_IMAGE_LINK}" title="{rss_image.RSS_IMAGE_TITLE}"><img src="{rss_image.RSS_IMAGE_URL}" alt="{rss_image.RSS_IMAGE_TITLE}" border="0" /></a>
  </td>
</tr> 
<!-- END rss_image -->

<!-- BEGIN rss_items -->
<tr> 
  <td class="row1" align="left"><a href="{rss_items.RSS_ITEM_LINK}" class="genmed target="_blank">{rss_items.RSS_ITEM_TITLE}</a></td> 
</tr> 
<!-- END rss_items -->

</tr> 

</table> 
<br clear="all" />
