					<!-- BEGIN menurow -->					

					
						<i class="nav-button{catrow.menurow.CURRENT}" onmouseout="this.className='nav-button{menurow.CURRENT}';" onmouseover="this.className='nav-button-current';">{catrow.menurow.U_MENU_ICON}<a href="{catrow.menurow.U_MENU_URL}" target="{catrow.menurow.U_MENU_URL_TARGET}">{catrow.menurow.MENU_NAME}</a></i>
					
					<!-- END menurow -->					
					<!-- END catrow -->
	</ul>
	</div>
</div>
